{
  "name": "generator-blennder",
  "version": "1.4.1",
  "description": "Generator for Blennder Theme Components",
  "homepage": "https://github.com/blenndiris/blennder",
  "author": {
    "name": "Blennd",
    "email": "ninjas@blannd.com",
    "url": "https://blennd.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "blennd",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "lodash": "^4.17.11",
    "nsp": "^3.2.1",
    "prettier": "^1.16.4",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "rxjs-compat": "^6.4.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "license": "Apache-2.0"
}
